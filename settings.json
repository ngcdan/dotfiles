{
  "editor.renderWhitespace": "all",
  "editor.wordWrapColumn": 120,
  "editor.formatOnPaste": false,
  "editor.rulers": [
    120
  ],
  "editor.autoIndent": "advanced",
  "editor.minimap.enabled": false,
  "editor.fontSize": 15.5,
  "editor.fontFamily": "JetBrains Mono",
  "editor.fontLigatures": true,
  "editor.tabSize": 2,
  "editor.tabCompletion": "on",
  "editor.formatOnSave": true,
  "editor.suggestSelection": "first",
  "editor.cursorStyle": "line",
  "editor.lineNumbers": "on",
  "editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-",
  "editor.wordWrap": "wordWrapColumn",
  "editor.detectIndentation": false,
  "files.autoSave": "onFocusChange",
  "files.trimTrailingWhitespace": true,
  "vim.easymotion": true,
  "vim.incsearch": true,
  "vim.useSystemClipboard": true,
  "vim.useCtrlKeys": true,
  "vim.hlsearch": true,
  "vim.leader": "<space>",
  "vim.sneak": true,
  "vim.insertModeKeyBindings": [
    {
      "before": [
        "j",
        "k"
      ],
      "after": [
        "<Esc>"
      ]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": [
        "<C-n>"
      ],
      "commands": [
        ":nohl"
      ]
    },
    {
      "before": [
        "<leader>",
        "w"
      ],
      "commands": [
        "workbench.action.splitEditor"
      ]
    },
    {
      "before": [
        "<leader>",
        "e"
      ],
      "commands": [
        "workbench.action.toggleSidebarVisibility"
      ]
    },
    {
      "before": [
        "<leader>",
        "f"
      ],
      "commands": [
        "revealInExplorer"
      ]
    },
    {
      "before": [
        "<leader>",
        "k"
      ],
      "after": [
        "/",
        "<C-R>",
        "<C-W>",
        "<CR>"
      ]
    },
    {
      "before": [
        "<leader>",
        "s"
      ],
      "after": [
        "<leader>",
        "<leader>",
        "/"
      ]
    },
    {
      "before": [
        "s"
      ],
      "after": [
        "<leader>",
        "<leader>",
        "2",
        "s"
      ]
    },
    {
      "before": [
        ":"
      ],
      "commands": [
        "workbench.action.showCommands"
      ]
    },
    {
      "before": [
        "leader",
        "w"
      ],
      "commands": [
        "workbench.action.files.save"
      ]
    },
    {
      "before": [
        "q",
        "q"
      ],
      "commands": [
        "workbench.action.closeActiveEditor",
      ]
    },
  ],
  "vim.normalModeKeyBindings": [
    {
      "before": [
        "<leader>",
        "d"
      ],
      "after": [
        "d",
        "d"
      ]
    },
    {
      "before": [
        "<tab>"
      ],
      "commands": [
        "workbench.action.nextEditor"
      ]
    },
    {
      "before": [
        "<S-tab>"
      ],
      "commands": [
        "workbench.action.previousEditor"
      ]
    },
  ],
  "vim.visualModeKeyBindings": [
    {
      "before": [
        ">"
      ],
      "after": [
        ">",
        "g",
        "v"
      ]
    },
    {
      "before": [
        "<"
      ],
      "after": [
        "<",
        "g",
        "v"
      ]
    },
    {
      "before": [
        ">"
      ],
      "commands": [
        "editor.action.indentLines"
      ]
    },
    {
      "before": [
        "<"
      ],
      "commands": [
        "editor.action.outdentLines"
      ]
    },
  ],
  "vim.statusBarColorControl": true,
  "vim.statusBarColors.normal": [
    "#a89984",
  ],
  "vim.handleKeys": {
    "<C-a>": false,
    "<C-f>": false,
    // VS Code new marker @ next occurence
    "<C-c>": false,
    // Cut
    "<C-x>": false,
    // Paste
    "<C-v>": false,
    "<C-z>": false,
    "<C-y>": false
  },
  "vim.vimrc.enable": true,
  "vim.statusBarColors.insert": "#83a598",
  "vim.statusBarColors.visual": "#fe8019",
  "vim.statusBarColors.visualline": "#fe8019",
  "vim.statusBarColors.visualblock": "#fe8019",
  "vim.statusBarColors.replace": "#689d6a",
  "vim.statusBarColors.commandlineinprogress": "#007ACC",
  "vim.statusBarColors.searchinprogressmode": "#007ACC",
  "vim.statusBarColors.easymotionmode": "#007ACC",
  "vim.statusBarColors.easymotioninputmode": "#007ACC",
  "vim.statusBarColors.surroundinputmode": "#007ACC",
  "terminal.external.osxExec": "kitty.app",
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.fontFamily": "JetBrains Mono",
  "terminal.integrated.fontSize": 13.5,
  "terminal.integrated.lineHeight": 1,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/node_modules": true
  },
  "[javascript]": {
    "editor.tabSize": 2
  },
  "[typescriptreact]": {
    "editor.tabSize": 2
  },
  "[typescript]": {
    "editor.tabSize": 2
  },
  "typescript.updateImportsOnFileMove.enabled": "always",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "emmet.triggerExpansionOnTab": true,
  "explorer.confirmDragAndDrop": false,
  "workbench.editor.labelFormat": "short",
  "explorer.confirmDelete": false,
  "breadcrumbs.enabled": true,
  "workbench.startupEditor": "newUntitledFile",
  "workbench.iconTheme": "material-icon-theme",
  "git.autofetch": true,
  "diffEditor.renderSideBySide": false,
  "search.exclude": {
    "": true,
    "**.map": true,
    "**.d.ts": true,
    "**/node_modules": true,
    "**/.git/**": true,
    "**/host/**": true,
    "host/**": true
  },
  "security.workspace.trust.untrustedFiles": "open",
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/node_modules/**": true
  },
  "workbench.colorCustomizations": {
    "statusBar.background": "#fe8019",
    "statusBar.noFolderBackground": "#fe8019",
    "statusBar.debuggingBackground": "#fe8019",
    "statusBar.foreground": "#434C5E",
    "statusBar.debuggingForeground": "#434C5E"
  },
  "zenMode.hideStatusBar": false,
  "gitlens.statusBar.enabled": false,
  "tabnine.experimentalAutoImports": true,
  "window.zoomLevel": -1.5,
  "cSpell.userWords": [
    "containertype",
    "datatp",
    "vgrid",
    "wentity"
  ],
  "gitlens.mode.active": "zen",
  "workbench.colorTheme": "One Dark Pro Darker"
}
